        },
        {
            "name": "Shell parameters",
            "scope": ["variable.parameter.option.shell"],
            "settings": {
                "foreground": "#D9A95D"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "#5DB1D9",
        "function": "#4A78B3",
        "variable": "#2A1F28",
        "string": "#6BAD9A",
        "number": "#D9A95D",
        "keyword": "#B15D95",
        "type": "#B15D95",
        "class": "#B15D95",
        "comment": "#AA8699",
        "parameter": "#D9A95D",
        "operator": "#5D92D9",
        "decorator": "#D95D92",
        "namespace": "#B15D95",
        "macro": "#D95D92",
        "enum": "#B15D95",
        "interface": "#B15D95",
        "struct": "#B15D95",
        "enum.member": "#D9A95D",
        "boolean": "#D95D92",
        "regexp": "#6BAD9A",
        "modifier": "#B15D95",
        "typeParameter": "#D9A95D",
        "method": "#4A78B3",
        "enumMember": "#D9A95D"
    }
}
