	"tokenColors": [
	  {
		"name": "Comments",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#6272A4"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#BD93F9"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#FF5555"
		}
	  },
	  {
		"name": "Keywords",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#FF79C6"
		}
	  },
	  {
		"name": "Operators",
		"scope": "keyword.operator",
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Strings",
		"scope": ["string", "constant.other.symbol"],
		"settings": {
		  "foreground": "#F1FA8C"
		}
	  },
	  {
		"name": "Functions",
		"scope": ["entity.name.function", "meta.function-call"],
		"settings": {
		  "foreground": "#50FA7B"
		}
	  },
	  {
		"name": "Classes",
		"scope": ["entity.name.type", "entity.name.class", "entity.name.namespace"],
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Numbers, Constants",
		"scope": ["constant.numeric", "constant.language", "constant.character"],
		"settings": {
		  "foreground": "#BD93F9"
		}
	  },
	  {
		"name": "Tags",
		"scope": ["entity.name.tag", "meta.tag.sgml"],
		"settings": {
		  "foreground": "#FF79C6"
		}
	  },
	  {
		"name": "Attributes",
		"scope": "entity.other.attribute-name",
		"settings": {
		  "foreground": "#50FA7B"
		}
	  },
	  {
		"name": "Markup Headings",
		"scope": "markup.heading",
		"settings": {
		  "fontStyle": "bold",
		  "foreground": "#FF79C6"
		}
	  },
	  {
		"name": "Markup Bold",
		"scope": "markup.bold",
		"settings": {
		  "fontStyle": "bold",
		  "foreground": "#FF79C6"
		}
	  },
	  {
		"name": "Markup Italic",
		"scope": "markup.italic",
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#BD93F9"
		}
	  },
	  {
		"name": "Markup Links",
		"scope": ["markup.underline.link", "string.other.link"],
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Markup Lists",
		"scope": "markup.list",
		"settings": {
		  "foreground": "#F1FA8C"
		}
	  },
	  {
		"name": "Markup Quotes",
		"scope": "markup.quote",
		"settings": {
		  "foreground": "#50FA7B",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Property Names",
		"scope": "support.type.property-name",
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Language Methods",
		"scope": "support.function",
		"settings": {
		  "foreground": "#50FA7B"
		}
	  },
	  {
		"name": "CSS Classes",
		"scope": "entity.other.attribute-name.class",
		"settings": {
		  "foreground": "#50FA7B"
		}
	  },
	  {
		"name": "CSS IDs",
		"scope": "entity.other.attribute-name.id",
		"settings": {
		  "foreground": "#FFB86C"
		}
	  },
	  {
		"name": "CSS Properties",
		"scope": "support.type.property-name.css",
		"settings": {
		  "foreground": "#8BE9FD"
		}
	  },
	  {
		"name": "Control Flow",
		"scope": "keyword.control",
		"settings": {
		  "foreground": "#FF79C6"
		}
	  }
	]
