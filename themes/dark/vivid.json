            "settings": {
                "foreground": "#FDA4BA"
            }
        },
        {
            "name": "Shell commands",
            "scope": ["support.function.builtin.shell"],
            "settings": {
                "foreground": "#7EB0E8"
            }
        },
        {
            "name": "Shell parameters",
            "scope": ["variable.parameter.option.shell"],
            "settings": {
                "foreground": "#F5D6A0"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "#78B3CD",
        "function": "#7EB0E8",
        "variable": "#D9D9D9",
        "string": "#9CDBA8",
        "number": "#F5D6A0",
        "keyword": "#FDA4BA",
        "type": "#D592BC",
        "class": "#D592BC",
        "comment": "#89888C",
        "parameter": "#F5D6A0",
        "operator": "#BF9180",
        "decorator": "#FDA4BA",
        "namespace": "#D592BC",
        "macro": "#FDA4BA",
        "enum": "#D592BC",
        "interface": "#D592BC",
        "struct": "#D592BC",
        "enum.member": "#F5D6A0",
        "boolean": "#FDA4BA",
        "regexp": "#9CDBA8",
        "modifier": "#FDA4BA",
        "typeParameter": "#F5D6A0",
        "method": "#7EB0E8",
        "enumMember": "#F5D6A0"
    }
}
